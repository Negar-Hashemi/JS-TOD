{
  "name": "js-tod",
  "version": "1.4.0",
  "description": "JS-TOD: A tool for detecting order-dependent flaky tests in Jest-based JavaScript projects.",
  "keywords": ["testing", "flaky tests", "order-dependent", "jest", "javascript"],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Negar-Hashemi/JS-TOD"
  },
  "bin": { "js-tod": "./reorderRunner.js" },
  "scripts": {
    "js-tod:run": "node reorderRunner.js",
    "js-tod:run:suite": "node reorderRunner.js --level=suite",
    "js-tod:run:describe": "node reorderRunner.js --level=describe",
    "js-tod:run:test": "node reorderRunner.js --level=test"
  },
  "dependencies": {
    "@babel/parser": "^7.22.10",
    "@babel/traverse": "^7.22.10",
    "@babel/generator": "^7.22.10",
    "is-utf8": "^0.2.1",
    "fs-extra": "^11.1.1",
    "buffer": "^6.0.3",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "engines": {
    "node": ">=18.17.0"
  }
}